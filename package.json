{
    "name": "goins.io",
    "version": "0.1.0",
    "description": "Motivational currency for GoITeens.",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon",
        "compile-sass": "node-sass assets/sass --output public/css --output-style compressed --indented-syntax --indented-type space --indented-width 4 --linefeed lf --source-map-embed",
        "compile-sass-watch": "node-sass --watch assets/sass --output public/css --output-style compressed --indented-syntax --indented-type space --indented-width 4 --linefeed lf --source-map-embed",
        "build": "babel src --out-dir dist",
        "start": "npm run build && node dist/index.js",
        "tern-definitions": "scripts/tern-definitions.sh",
        "update-bootstrap": "scripts/update-bootstrap.sh",
        "jsdoc": "jsdoc src --configure .jsdoc.json"
    },
    "keywords": [],
    "author": "FederationOfCoders.org",
    "license": "Apache-2.0",
    "dependencies": {
        "babel-runtime": "^6.20.0",
        "body-parser": "^1.15.2",
        "clean-css": "^3.4.23",
        "cookie-parser": "^1.4.3",
        "express": "^4.14.0",
        "html-minifier": "^3.2.3",
        "method-override": "^2.3.7",
        "mongodb": "^2.2.19",
        "morgan": "^1.7.0",
        "nunjucks": "^3.0.0",
        "relateurl": "^0.2.7",
        "uglifyjs": "^2.4.10",
        "underscore": "^1.8.3"
    },
    "devDependencies": {
        "babel-cli": "^6.11.0",
        "babel-eslint": "^7.1.1",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-es2015": "^6.11.0",
        "babel-preset-stage-0": "^6.16.0",
        "eslint": "^3.12.2",
        "ink-docstrap": "^1.3.0",
        "jsdoc": "^3.4.3",
        "node-sass": "^4.1.1",
        "nodemon": "^1.11.0",
        "tern": "^0.20.0"
    }
}
